			// 이어보기 시간
			continuerTime	 : 0,
			// 버퍼시간 설정(기본값 : 1.5)
			bufferTime       : 1.5,
			// 사용할 컨트롤러들 설정
			controllers      : {
				timeSlider      : true, // 타임 슬라이더
				playButton      : true, // 재생/일시정지 버튼
				osdPlayButton   : false, // OSD 영역의 재생/일시정지 버튼
				currentTime     : true, // 현재 재생시간 라벨
				durationTime    : true, // 총 재생시간 라벨
				fullscreenButton: true, // 풀스크린 버튼
				expandButton    : true, // 화면확장 버튼
				volumeButton    : true, // 볼륨 버튼
				volumeSlider    : true, // 볼륨 슬라이더
			    speedDownButton : true, // 배속 감소 버튼
				currentSpeed    : true, // 현재 배속 라벨
		        speedUpButton   : true // 배속 증가 버튼
			},

			speedLabelText : ' x',

			speedRates : [0.5, 1.0, 1.5, 2.0, 2.5, 3.0],

			progressTime : true,

			useShortcuts : true,

		   // 외부 API
			externalAPI     : {
				modeCollapse    : collapseVideo,
				modeExpand      : expandVideo
			},

			// 컨트롤바 자동 숨김 사용 여부
			controllersAutoHide : false,
			controllerHideDelay : 3,

			// 플레이어 사용 도메인
			domain : 'sw.wowtv.co.kr',

			// 디버그 모드
			// 배포시에 false 지정 또는 제거
			// 개발 및 디버그시 true로 지정
			//debugMode        : true,
			// 리소스
			sources          : null,
			// 스킨 (eduwill.css)
			skin             : 'wowtv',
			// 볼륨 슬라이더 유형
			volumeSliderType : 1
		};
		////////////////////////////////////////////////////////////////////////////////

		/// DOM Ready //////////////////////////////////////////////////////////////////
		/*
		 * 해당 페이지의 DOM이 준비된 후 호출되는 메서드로
		 * 리소스를 지정하고 플레이어 인스턴스 객체를 생성
		 */
		function onDOMReady() {
			// 모바일이 아닐 경우 리소스 설정
			if (!mp.browser.isMobile) {
				cfg.poster = '';
				cfg.sources = [
				// 광고
		//        {
		//            isAD       : true,
		//            skipTime   : 5,
		//            clickToURL : '/funfun/',
